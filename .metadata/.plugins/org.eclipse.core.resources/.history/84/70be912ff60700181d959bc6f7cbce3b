package common;



public static bool isClass() {
	return true;
}

String toHangul(String s){
	String changeString = s;
	try{
		if (s != null) {
			changeString= new String(s.getBytes("8859_1"),"utf-8");
		}
			
	} catch (Exception e) {

	}
	return changeString;
}

public class Check {

	String toHangul(String s){
		String changeString = s;
		try{
			if (s != null) {
				changeString= new String(s.getBytes("8859_1"),"utf-8");
			}
				
		} catch (Exception e) {

		}
		return changeString;
	}
}
